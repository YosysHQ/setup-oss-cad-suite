    "@types/node": "^18.7.16",
		"@typescript-eslint/eslint-plugin": "^5.36.2",
		"@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-plugin-github": "^4.3.2",
    "typescript": "^4.4.4"
